{
    "name": "cetakbiru-flask",
    "version": "0.1.0",
    "engines": {
        "node": ">= 2.18.1",
        "npm": ">= 6.14.5"
    },
    "description": "Boilerplate aplikasi Flask",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/desa-id/cetakbiru-flask.git"
    },
    "author": "Aris Ripandi <riipandi@gmail.com> (github.com/riipandi)",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/desa-id/cetakbiru-flask/issues"
    },
    "homepage": "https://github.com/desa-id/cetakbiru-flask#readme",
    "scripts": {
        "clean": "rimraf ./static/assets",
        "build": "npm run clean && parcel build static/js/application.js -d static/assets --public-url . --cache-dir .parcel-cache",
        "watch": "npm run clean && parcel watch static/js/application.js -d static/assets --public-url . --cache-dir .parcel-cache",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "devDependencies": {
        "autoprefixer": "^9.8.1",
        "rimraf": "^3.0.2",
        "parcel-bundler": "^1.12.4"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.13.1",
        "axios": "^0.19.2",
        "bootstrap": "^4.5.0",
        "jquery": "^3.5.1",
        "node-sass": "^4.14.1",
        "popper.js": "^1.16.1"
    },
    "heroku-run-build-script": true
}
